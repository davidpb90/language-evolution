@article{size,
	title = {The meta book and size-dependent properties of written language},
	volume = {11},
	issn = {1367-2630},
	url = {http://iopscience.iop.org/1367-2630/11/12/123015},
	doi = {10.1088/1367-2630/11/12/123015},
	abstract = {Evidence is presented for a systematic text-length dependence of the power-law index γ of a single book. The estimated γ values are consistent with a monotonic decrease from 2 to 1 with increasing text length. A direct connection to an extended Heap's law is explored. The infinite book limit is, as a consequence, proposed to be given by γ=1 instead of the value γ=2 expected if Zipf's law is universally applicable. In addition, we explore the idea that the systematic text-length dependence can be described by a meta book concept, which is an abstract representation reflecting the word-frequency structure of a text. According to this concept the word-frequency distribution of a text, with a certain length written by a single author, has the same characteristics as a text of the same length extracted from an imaginary complete infinite corpus written by the same author.},
	language = {en},
	number = {12},
	urldate = {2015-06-18},
	journal = {New Journal of Physics},
	author = {Bernhardsson, Sebastian and Rocha, Luis Enrique Correa da and Minnhagen, Petter},
	month = dec,
	year = {2009},
	pages = {123015},
	file = {Full Text PDF:/Users/cepardot/Library/Application Support/Zotero/Profiles/ftzoo3mp.default/zotero/storage/8ZNWCN95/Bernhardsson et al. - 2009 - The meta book and size-dependent properties of wri.pdf:application/pdf;Snapshot:/Users/cepardot/Library/Application Support/Zotero/Profiles/ftzoo3mp.default/zotero/storage/CRAWD7PJ/fulltext.html:text/html}
