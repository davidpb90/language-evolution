	pages = {83--87},
	file = {Full Text PDF:/Users/cepardot/Library/Application Support/Zotero/Profiles/ftzoo3mp.default/zotero/storage/H96HTFIG/Roivainen - 2014 - Changes in Word Usage Frequency May Hamper Interge.pdf:application/pdf;Snapshot:/Users/cepardot/Library/Application Support/Zotero/Profiles/ftzoo3mp.default/zotero/storage/KHIG59EH/83.html:text/html}
